        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="获取加密encpw" enabled="true">
          <stringProp name="BeanShellSampler.query">import java.math.BigInteger;
import java.security.SecureRandom;




public BigInteger pkcs1pad2(String text,BigInteger eValue) {
//   log.info(&quot;pkcs1pad2 start&quot;);
//   log.info(String.valueOf(eValue));
//   log.info(text);
   int eLength = (eValue.bitLength() + 7) &gt;&gt; 3;
   if (text.length() + 11 &gt; eLength) {
       return null;
   }
//   log.info(&quot;--------&quot;);
//   log.info(String.valueOf(eLength));
   byte[] bytes = new byte[eLength];
   int sLength = text.length() - 1;
//   log.info(&quot;++__++&quot;);
//   log.info(String.valueOf(sLength));
   while (sLength &gt;= 0 &amp;&amp; eLength &gt; 0) {
       bytes[--eLength] = (byte) text.charAt(sLength--);
   }
//   log.info(&quot;{{{{&quot;);
//   log.info(String.valueOf(eLength));
   bytes[--eLength] = (byte)0;
//   log.info(&quot;}}}}&quot;);
   SecureRandom random = null;
//   log.info(&quot;++++++&quot;);
   try {
       random = SecureRandom.getInstance(&quot;SHA1PRNG&quot;);
   } catch (Exception e) {
       return null;
   }
//   log.info(&quot;======&quot;);
   byte[] temp = new byte[2000];
   while (eLength &gt; 2) {
       temp[0] = (byte)0;
       while (temp[0] == (byte)0) {
           random.nextBytes(temp);
       }
       bytes[--eLength] = temp[0];
   }
   bytes[--eLength] = (byte)2;
   bytes[--eLength] = (byte)0;
   return new BigInteger(bytes);
}


public String encrypt(String text,String nalue,String ealue) {
   String nalue1 = nalue.toLowerCase();
   String ealue1 = ealue.toLowerCase();
//   log.info(nalue);
//   log.info(ealue);
   BigInteger eValue = new BigInteger(ealue1, 16);
   BigInteger nValue = new BigInteger(nalue1, 16);
//   log.info(&quot;BIGINT&quot;);

   BigInteger value = pkcs1pad2(text,eValue);
//   log.info(&quot;value&quot;);
//   log.info(String.valueOf(value));
   if (value == null) {
       return null;
   }

   
   value = value.modPow(nValue, eValue);
   if (value == null) {
       return null;
   }
   String result = value.toString(16);
   if ((result.length() &amp; 1) == 0) {
       return result;
   } else {
       return &quot;0&quot; + result;
   }
}


public static String getEncpw(String id, String pwd, String sessionKey,String eValue, String nValue) {

   String text = (char) sessionKey.length() + sessionKey;
//   log.info(text);
   if (id != null) {
       text = text + (char) id.length() + id;
   }
//   log.info(text);
   if (pwd != null) {
       text = text + (char) pwd.length() + pwd;
   }
//   log.info(text);
//   log.info(&quot;getEncpw&quot;);
//   log.info(text);
   return encrypt(text,nValue,eValue);
}


